// 显示默认值
$accentColor = rgb(83, 168, 255)
$textColor = #2c3e50
$borderColor = #eaecef
$codeBgColor = #282c34

.theme-default-content code {
    word-break: break-word;
    border-radius: 2px;
    overflow-x: auto;
    background-color: #fff5f5;
    color: #ff502c;
    font-size: .87em;
    padding: 0.065em 0.4em;
}